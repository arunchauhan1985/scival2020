<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:tns="http://www.elsevier.com/xml/schema/grant/grant-3.0"
    targetNamespace="http://www.elsevier.com/xml/schema/grant/grant-3.0" elementFormDefault="qualified" attributeFormDefault="unqualified">

    <!--<include schemaLocation="common30.xsd"/>-->
    <import namespace="http://www.w3.org/XML/1998/namespace" schemaLocation="xml.xsd"/>
    
    <annotation>
        <documentation> This schema represents a past award within SciVal Funding. Author: Sanjeev Rao s.rao@paritycomputing.com </documentation>
        <documentation> This version 1.2 of the schema updated by MRu, 30-Dec-10 </documentation>
        <documentation>Further updates to 1.2 in February 2011 and July 2012 (JMi)</documentation>
        <documentation>Version 1.4.0p1 (JMi, 29 January 2014)</documentation>
        <documentation>Version 2.0.0 (JMi, 20 October 2014)</documentation>
        <documentation>Version 3.0.0r2 (JMi, 21 February 2017)></documentation>
    </annotation>

    <element name="awards" type="tns:AwardsType"/>

    <!-- oooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooo -->
    <complexType name="AwardsType">
        <sequence>
            <element name="award" type="tns:AwardType" maxOccurs="unbounded"/>
        </sequence>
    </complexType>

    <!-- oooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooo -->
    <complexType name="AwardType">
        <sequence>
            <element name="revisionHistory" type="tns:StatusAndRevisionHistoryType" nillable="true" minOccurs="0"/>
            <element name="recordSource" type="string" minOccurs="0">
                <annotation>
                    <documentation>Source used to obtain award information.</documentation>
                </annotation>
            </element>
            <element name="name" type="tns:StringWithLanguageType" maxOccurs="unbounded"/>
            <element name="NIHData" type="tns:NIHDataType" minOccurs="0">
                <annotation>
                    <documentation>NIH RePORTER additions.</documentation>
                </annotation>
            </element>
            <element name="orgCountry" type="string" minOccurs="0"/>
            <element name="startDate" type="date" nillable="true" minOccurs="0">
                <annotation>
                    <documentation>Logical start date from external source</documentation>
                </annotation>
            </element>
            <element name="lastAmendedDate" type="date" nillable="true" minOccurs="0">
                <annotation>
                    <documentation>Logical last amended date from external source</documentation>
                </annotation>
            </element>
            <element name="endDate" type="date" nillable="true" minOccurs="0">
                <annotation>
                    <documentation>Logical end date from external source</documentation>
                </annotation>
            </element>
            <element name="amount" type="tns:AmountType" nillable="true" minOccurs="0"/>
            <element name="abstract" type="tns:StringWithLanguageType" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
            <element name="classificationGroup" type="tns:ClassificationGroupType" nillable="true" minOccurs="0"/>
            <element name="keywords" type="tns:KeywordsType" nillable="true" minOccurs="0"/>
            <element name="awardees" type="tns:AwardeesType" nillable="true" minOccurs="0"/>
            <element name="awardManagers" type="tns:AwardManagersType" nillable="true" minOccurs="0"/>
            <element name="relatedPrograms" type="tns:RelatedProgramsType" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
            <element name="relatedFundingBodies" type="tns:MicroRelatedFundingBodiesType" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
            <element name="relatedItems" type="tns:MiniRelatedItemsType" minOccurs="0"/>
            <element name="scholarlyOutput" type="tns:ScholarlyOutputType" minOccurs="0"/>
        </sequence>
        <attribute name="id" type="integer" use="required"/>
        <attribute name="fundingBodyAwardId" type="string" use="required"/>
        <attribute name="type" type="tns:FundingTypeIDsType" use="required"/>
        <attribute name="trusting" type="boolean" use="optional"/>
        <attribute name="collectionCode" type="string" use="optional"/>
        <attribute name="hidden" type="boolean" default="false" use="optional"/>
    </complexType>

    <complexType name="NIHDataType">
        <sequence>
            <element name="applicationID" type="string" minOccurs="0"/>
            <element name="FOANumber" type="string" minOccurs="0">
                <annotation>
                    <documentation>Funding Opportunity Announcement Number.</documentation>
                </annotation>
            </element>
            <element name="fundingICs" type="string" minOccurs="0">
                <annotation>
                    <documentation>Funding Institute/Center. See also http://grants.nih.gov/grants/glossary.htm#I</documentation>
                </annotation>
            </element>
            <element name="orgDuns" type="string" minOccurs="0">
                <annotation>
                    <documentation>The unique number provided by Dun &amp; Bradstreet to all Federal government contractors and grantees in US. See
                        also http://fedgov.dnb.com/webform/</documentation>
                </annotation>
            </element>
            <element name="orgDept" type="string" minOccurs="0"/>
            <element name="piID" type="string" minOccurs="0">
                <annotation>
                    <documentation>Principal Investigator ID assigned by NIH to awardee.</documentation>
                </annotation>
            </element>
            <element name="projectStartDate" type="date" nillable="true" minOccurs="0"/>
            <element name="projectEndDate" type="date" nillable="true" minOccurs="0"/>
            <element name="supportYears" type="positiveInteger" minOccurs="0">
                <annotation>
                    <documentation>Number of years this NIT award is supported. Minimum is 1.</documentation>
                </annotation>
            </element>
            <element name="subProjectID" type="string" minOccurs="0"/>
            <element name="totalCostSubProject" type="tns:AmountType" nillable="true" minOccurs="0"/>
            <element name="coreProjectNumber" type="string" minOccurs="0"/>
            <element name="educationInstituteType" type="string" minOccurs="0"/>
            <element name="awardNoticeDate" type="date" minOccurs="0"/>
            <element name="studySection" type="string" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
        </sequence>
    </complexType>
    <!-- oooooooooooooooooooooooooooooooooooooooocommon30.xsdoooooooooooooooooooooooooooooooooooooo -->

    <complexType name="AddressType">
        <sequence>
            <element name="room" type="string" minOccurs="0"/>
            <element name="street" type="string" minOccurs="0"/>
            <element name="city" type="string"/>
            <element name="state" type="string" minOccurs="0"/>
            <element name="postalCode" type="string" minOccurs="0"/>
            <element name="country" type="string">
                <annotation>
                    <documentation>Country name as per ISO 3166. There will be no name variations. See
                        http://www.iso.org/iso/country_codes/iso_3166_code_lists/english_country_names_and_code_elements.htm</documentation>
                </annotation>
            </element>
        </sequence>
        <attribute name="country" type="tns:CountryCodeType" use="required">
            <annotation>
                <documentation>ISO 3166 country code</documentation>
            </annotation>
        </attribute>
    </complexType>

    <!-- oooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooo -->
    <complexType name="AffiliationsType">
        <sequence>
            <element name="affiliation" type="tns:AffiliationType" maxOccurs="unbounded"/>
        </sequence>
    </complexType>

    <!-- oooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooo -->
    <complexType name="AffiliationType">
        <sequence>
            <element name="org" type="string"/>
            <element name="dept" type="string" minOccurs="0"/>
            <element name="startDate" type="date" minOccurs="0"/>
            <element name="endDate" type="date" minOccurs="0"/>
            <element name="address" type="tns:AddressType" minOccurs="0" maxOccurs="unbounded"/>
            <element name="telephone" type="string" minOccurs="0" maxOccurs="unbounded"/>
            <element name="fax" type="string" minOccurs="0" maxOccurs="unbounded"/>
            <element name="email" type="string" minOccurs="0"/>
            <element name="webpage" type="string" minOccurs="0"/>
        </sequence>
        <attribute name="ScopusInstitutionID" type="integer" use="optional"/>
    </complexType>

    <!-- oooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooo -->
    <simpleType name="AmountDecimalType">
        <restriction base="decimal">
            <fractionDigits value="2"/>
        </restriction>
    </simpleType>

    <!-- oooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooo -->
    <complexType name="AmountType">
        <simpleContent>
            <extension base="tns:IntegerAndNotSpecifiedType">
                <attribute name="currency" type="tns:CurrencyType" use="optional" default="USD"/>
            </extension>
        </simpleContent>
    </complexType>
    <simpleType name="IntegerAndNotSpecifiedType">
        <union memberTypes="integer tns:NotSpecifiedType"/>
    </simpleType>

    <!-- oooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooo -->
    <complexType name="AwardeeNameType">
        <sequence>
            <element name="indexedName" type="string"/>
            <element name="givenName" type="string" minOccurs="0"/>
            <element name="initials" type="string" minOccurs="0"/>
            <element name="surname" type="string" minOccurs="0"/>
        </sequence>
    </complexType>

    <!-- oooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooo -->
    <complexType name="AwardeesType">
        <sequence>
            <element name="awardee" type="tns:AwardeeType" maxOccurs="unbounded"/>
        </sequence>
    </complexType>

    <!-- oooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooo -->
    <complexType name="AwardeeType">
        <sequence>
            <element name="awardeeName" type="tns:AwardeeNameType"/>
            <element name="awardeeInstitution" type="tns:AffiliationsType"/>
        </sequence>
        <attribute name="id" type="string" use="optional"/>
        <attribute name="type" type="tns:AwardeeTypeType" use="required"/>
        <attribute name="ScopusAuthorID" type="integer" use="optional"/>
    </complexType>

    <!-- oooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooo -->
    <simpleType name="AwardeeTypeType">
        <restriction base="string">
            <enumeration value="PI">
                <annotation>
                    <documentation>Primary Investigator</documentation>
                </annotation>
            </enumeration>
            <enumeration value="coPI">
                <annotation>
                    <documentation>Co Investigator</documentation>
                </annotation>
            </enumeration>
            <enumeration value="investigator">
                <annotation>
                    <documentation>Investigator. PI and coPIs are legally responsible and typically are senior people. An investigator is an
                        acknowledged helping hand who has no legal responsibility.</documentation>
                </annotation>
            </enumeration>
            <enumeration value="institution">
                <annotation>
                    <documentation>An institution as awardee.</documentation>
                </annotation>
            </enumeration>
        </restriction>
    </simpleType>

    <!-- oooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooo -->
    <complexType name="AwardManagersType">
        <sequence>
            <element name="awardManager" type="tns:ContactType" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
        </sequence>
    </complexType>

    <!-- oooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooo -->
    <complexType name="AwardStatisticsType">
        <sequence>
            <element name="totalFunding" type="tns:AmountType" nillable="true" minOccurs="0" maxOccurs="1"/>
            <element name="link" type="tns:LinkType" nillable="true" minOccurs="0" maxOccurs="1"/>
        </sequence>
    </complexType>

    <!-- oooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooo -->
    <simpleType name="BinaryRelationType">
        <restriction base="string">
            <enumeration value="about"/>
            <enumeration value="acknowledgedBy"/>
            <enumeration value="administeredBy"/>
            <enumeration value="affiliatedWith"/>
            <enumeration value="applicationInfoFor"/>
            <enumeration value="backgroundInfoFor"/>
            <enumeration value="continuationOf"/>
            <enumeration value="continuedAs"/>
            <enumeration value="formerlyIncluding"/>
            <enumeration value="formerlyKnownAs"/>
            <enumeration value="fundedBy"/>
            <enumeration value="grantedBy"/>
            <enumeration value="incorporatedInto"/>
            <enumeration value="incorporates"/>
            <enumeration value="isRelatedTo"/>
            <enumeration value="isReplacedBy"/>
            <enumeration value="issuedBy"/>
            <enumeration value="jointlyOwnedBy"/>
            <enumeration value="mergedWith"/>
            <enumeration value="mergerOf"/>
            <enumeration value="operatedBy"/>
            <enumeration value="originatedFrom"/>
            <enumeration value="partOf"/>
            <enumeration value="relevantTo"/>
            <enumeration value="renamedAs"/>
            <enumeration value="replaces"/>
            <enumeration value="resultedIn"/>
            <enumeration value="similarTo"/>
            <enumeration value="solicitedBy"/>
            <enumeration value="splitFrom"/>
            <enumeration value="splitInto"/>
            <enumeration value="sponsoredBy"/>
        </restriction>
    </simpleType>
    <simpleType name="OpportunitiesRelationsType">
        <restriction base="tns:BinaryRelationType">
            <enumeration value="isRelatedTo"/>
            <enumeration value="isReplacedBy"/>
            <enumeration value="replaces"/>
        </restriction>
    </simpleType>
    <simpleType name="ScholarlyOutputRelationsType">
        <restriction base="tns:BinaryRelationType">
            <enumeration value="resultedIn"/>
        </restriction>
    </simpleType>

    <!-- oooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooo -->
    <complexType name="ChangeHistoryType">
        <sequence>
            <element name="change" type="tns:ChangeType" maxOccurs="unbounded"/>
        </sequence>
    </complexType>

    <!-- oooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooo -->
    <complexType name="ChangeType">
        <simpleContent>
            <extension base="string">
                <annotation>
                    <documentation>This is the 'before' content, not 'after' content.</documentation>
                </annotation>
                <attribute name="type" type="string" use="required"/>
                <attribute name="postDate" type="date" use="required"/>
                <attribute name="version" type="integer" use="required"/>
            </extension>
        </simpleContent>
    </complexType>

    <!-- oooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooo -->
    <complexType name="ClassificationGroupType">
        <sequence minOccurs="0">
            <element name="classifications" type="tns:ClassificationsType" maxOccurs="unbounded"/>
        </sequence>
    </complexType>

    <!-- oooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooo -->
    <complexType name="ClassificationsType">
        <sequence>
            <element name="classification" type="tns:ClassificationType" maxOccurs="unbounded"/>
        </sequence>
        <attribute name="type" type="tns:ClassificationsTypeType" use="required"/>
    </complexType>

    <!-- oooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooo -->
    <simpleType name="ClassificationsTypeType">
        <restriction base="string">
            <enumeration value="ASJC"/>
            <enumeration value="CFDA"/>
            <enumeration value="INDEL"/>
            <enumeration value="Grants.gov"/>
            <enumeration value="NIHspendingCategory"/>
            <enumeration value="orgSpecific"/>
            <enumeration value="StudySection"/>
        </restriction>
    </simpleType>

    <!-- oooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooo -->
    <complexType name="ClassificationType">
        <simpleContent>
            <extension base="string">
                <attribute name="frequency" type="integer" use="required"/>
                <attribute name="code" type="string" use="optional"/>
            </extension>
        </simpleContent>
    </complexType>

    <!-- oooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooo -->
    <simpleType name="ClassificationTypeIDValues">
        <union memberTypes="tns:ClassificationTypeIDValuesType tns:NotSpecifiedType tns:CountryCodeType"/>
    </simpleType>
    <simpleType name="ClassificationTypeIDValuesType">
        <restriction base="token">
            <enumeration value="academic">
                <annotation>
                    <documentation>type='restrictions'; eligibilityClassification='Academic Institution'</documentation>
                </annotation>
            </enumeration>
            <enumeration value="commercial">
                <annotation>
                    <documentation>type='restrictions'; eligibilityClassification='Commercial Organization'</documentation>
                </annotation>
            </enumeration>
            <enumeration value="degree">
                <annotation>
                    <documentation>type='restrictions'; eligibilityClassification='Ph.D./M.D. other professional'</documentation>
                </annotation>
            </enumeration>
            <enumeration value="disabilities">
                <annotation>
                    <documentation>type='restrictions'; eligibilityClassification='Persons with Disabilities'</documentation>
                </annotation>
            </enumeration>
            <enumeration value="government">
                <annotation>
                    <documentation>type='restrictions'; eligibilityClassification='Local or State Government'</documentation>
                </annotation>
            </enumeration>
            <enumeration value="graduate">
                <annotation>
                    <documentation>type='restrictions'; eligibilityClassification='Graduate Student'</documentation>
                </annotation>
            </enumeration>
            <enumeration value="minorities">
                <annotation>
                    <documentation>type='restrictions'; eligibilityClassification='Underrepresented Minorities'</documentation>
                </annotation>
            </enumeration>
            <enumeration value="newfaculty">
                <annotation>
                    <documentation>type='restrictions'; eligibilityClassification='New Faculty/New Investigator'</documentation>
                </annotation>
            </enumeration>
            <enumeration value="nonprofit">
                <annotation>
                    <documentation>type='restrictions'; eligibilityClassification='Non Profit Organization'</documentation>
                </annotation>
            </enumeration>
            <enumeration value="norestr">
                <annotation>
                    <documentation>type='citizenship'; eligibilityClassification='No country-related restrictions'</documentation>
                </annotation>
            </enumeration>
            <enumeration value="regionspecific">
                <annotation>
                    <documentation>type='restrictions'; eligibilityClassification='Region restrictions'</documentation>
                </annotation>
            </enumeration>
            <enumeration value="sme">
                <annotation>
                    <documentation>type='restrictions'; eligibilityClassification='Small and Medium Enterprise'</documentation>
                </annotation>
            </enumeration>
            <enumeration value="undergraduate">
                <annotation>
                    <documentation>type='restrictions'; eligibilityClassification='Undergraduate Student'</documentation>
                </annotation>
            </enumeration>
            <enumeration value="women">
                <annotation>
                    <documentation>type='restrictions'; eligibilityClassification='Women'</documentation>
                </annotation>
            </enumeration>
        </restriction>
    </simpleType>

    <!-- oooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooo -->
    <complexType name="ContactListType">
        <sequence>
            <element name="contact" type="tns:ContactType" maxOccurs="unbounded"/>
        </sequence>
    </complexType>

    <!-- oooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooo -->
    <complexType name="ContactType">
        <sequence>
            <element name="contactName" type="tns:VisibleNameType" nillable="true" minOccurs="0"/>
            <element name="title" type="string" nillable="true" minOccurs="0"/>
            <element name="telephone" type="string" nillable="true" minOccurs="0"/>
            <element name="fax" type="string" nillable="true" minOccurs="0"/>
            <element name="email" type="string" nillable="true" minOccurs="0"/>
            <element name="website" type="tns:LinkType" nillable="true" minOccurs="0"/>
            <element name="address" type="tns:AddressType" nillable="true" minOccurs="0"/>
        </sequence>
        <attribute name="type" type="string" use="optional"/>
    </complexType>

    <!-- oooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooo -->
    <simpleType name="CountryCodeType">
        <restriction base="string">
            <pattern value="[a-z][a-z][a-z]"/>
        </restriction>
    </simpleType>

    <!-- oooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooo -->
    <annotation>
        <documentation> This schema represents currencies within the Grants Optimizer. Author: Sanjeev Rao s.rao@paritycomputing.com </documentation>
    </annotation>

    <simpleType name="CurrencyType">
        <union memberTypes="tns:CurrencyType-ISO4217 tns:CurrencyType-Legacy"/>
    </simpleType>
