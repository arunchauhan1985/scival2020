                </annotation>
            </enumeration>
            <enumeration value="BOV">
                <annotation>
                    <documentation>Bolivian Mvdol (funds code)</documentation>
                </annotation>
            </enumeration>
            <enumeration value="BRL">
                <annotation>
                    <documentation>Brazilian real</documentation>
                </annotation>
            </enumeration>
            <enumeration value="BSD">
                <annotation>
                    <documentation>Bahamian dollar</documentation>
                </annotation>
            </enumeration>
            <enumeration value="BTN">
                <annotation>
                    <documentation>Bhutanese ngultrum</documentation>
                </annotation>
            </enumeration>
            <enumeration value="BWP">
                <annotation>
                    <documentation>Botswana pula</documentation>
                </annotation>
            </enumeration>
            <enumeration value="BYN">
                <annotation>
                    <documentation>New Belarusian ruble</documentation>
                </annotation>
            </enumeration>
            <enumeration value="BYR">
                <annotation>
                    <documentation>Belarusian ruble</documentation>
                </annotation>
            </enumeration>
            <enumeration value="BZD">
                <annotation>
                    <documentation>Belize dollar</documentation>
                </annotation>
            </enumeration>
            <enumeration value="CAD">
                <annotation>
                    <documentation>Canadian dollar</documentation>
                </annotation>
            </enumeration>
            <enumeration value="CDF">
                <annotation>
                    <documentation>Congolese franc</documentation>
                </annotation>
            </enumeration>
            <enumeration value="CHE">
                <annotation>
                    <documentation>WIR Euro (complementary currency)</documentation>
                </annotation>
            </enumeration>
            <enumeration value="CHF">
                <annotation>
                    <documentation>Swiss franc</documentation>
                </annotation>
            </enumeration>
            <enumeration value="CHW">
                <annotation>
                    <documentation>WIR Franc (complementary currency)</documentation>
                </annotation>
            </enumeration>
            <enumeration value="CLF">
                <annotation>
                    <documentation>Unidad de Fomento (funds code)</documentation>
                </annotation>
