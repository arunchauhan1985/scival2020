    <!-- oooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooo -->
    <complexType name="EligibilityClassificationType">
        <annotation>
            <documentation>Attributes city, state and country are only to be used if attribute type="regionspecific"</documentation>
        </annotation>
        <simpleContent>
            <extension base="string">
                <attribute name="type" type="tns:TypeIDValuesAndNotSpecifiedType" use="required"/>
                <attribute name="id" type="tns:ClassificationTypeIDValues" use="required"/>
                <attribute name="city" type="string" use="optional"/>
                <attribute name="state" type="string" use="optional"/>
                <attribute name="country" type="tns:CountryCodeType" use="optional"/>
            </extension>
        </simpleContent>
    </complexType>

    <!-- oooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooo -->
    <complexType name="EstablishmentInfoType">
        <sequence>
            <element name="establishmentDate" type="gYear" nillable="true" minOccurs="0"/>
            <element name="establishmentCity" type="string" nillable="true" minOccurs="0"/>
            <element name="establishmentState" type="string" nillable="true" minOccurs="0"/>
            <element name="establishmentCountryCode" type="tns:CountryCodeType" nillable="true" minOccurs="0">
                <annotation>
                    <documentation>ISO 3166 country code</documentation>
                </annotation>
            </element>
            <element name="establishmentDescription" type="string" nillable="true" minOccurs="0"/>
        </sequence>
    </complexType>

    <!-- oooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooo -->
    <complexType name="FinancialInfoType">
        <sequence>
            <element name="taxIds" type="tns:TaxIDType" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
            <element name="fiscalCloseDate" type="gMonthDay" nillable="true" minOccurs="0" maxOccurs="unbounded">
                <annotation>
                    <documentation>See http://books.xmlschemata.org/relaxng/ch19-77119.html for examples</documentation>
                </annotation>
            </element>
        </sequence>
    </complexType>

    <!-- oooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooo -->
