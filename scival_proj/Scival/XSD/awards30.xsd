                <annotation>
                    <documentation>Azerbaijani manat</documentation>
                </annotation>
            </enumeration>
            <enumeration value="BAM">
                <annotation>
                    <documentation>Bosnia and Herzegovina convertible mark</documentation>
                </annotation>
            </enumeration>
            <enumeration value="BBD">
                <annotation>
                    <documentation>Barbados dollar</documentation>
                </annotation>
            </enumeration>
            <enumeration value="BDT">
                <annotation>
                    <documentation>Bangladeshi taka</documentation>
                </annotation>
            </enumeration>
            <enumeration value="BGN">
                <annotation>
                    <documentation>Bulgarian lev</documentation>
                </annotation>
            </enumeration>
            <enumeration value="BHD">
                <annotation>
                    <documentation>Bahraini dinar</documentation>
                </annotation>
            </enumeration>
            <enumeration value="BIF">
                <annotation>
                    <documentation>Burundian franc</documentation>
                </annotation>
            </enumeration>
            <enumeration value="BMD">
                <annotation>
                    <documentation>Bermudian dollar</documentation>
                </annotation>
            </enumeration>
            <enumeration value="BND">
                <annotation>
                    <documentation>Brunei dollar</documentation>
                </annotation>
