    <annotation>
        <documentation> This schema represents status and revision history of various types within the Grants Optimizer. Author: Sanjeev Rao
            s.rao@paritycomputing.com </documentation>
    </annotation>
    <complexType name="StatusAndRevisionHistoryType">
        <sequence>
            <element name="revisedDate" type="tns:RevisionDateType" minOccurs="0"/>
            <element name="createdDate" type="tns:RevisionDateType"/>
        </sequence>
        <attribute name="status" type="tns:StatusType" use="required"/>
    </complexType>

    <!-- oooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooo -->
    <simpleType name="StatusType">
        <annotation>
            <documentation> If StatusAndRevisionHistoryType/RevisionDateType/version == 1, status = 'new'. If version greater than 1, status =
                'update'. </documentation>
        </annotation>
        <restriction base="string">
            <enumeration value="delete"/>
            <enumeration value="new"/>
            <enumeration value="update"/>
        </restriction>
    </simpleType>

